syntax on

"set noerrorbells
set number relativenumber
set tabstop=4 softtabstop=4
set shiftwidth=4
set expandtab
set smartindent
set nowrap
"set smartcase
"set nobackup
"set no swapfile
"set undodir ~/.vim/undodir
"set undofile
set incsearch

call plug#begin('~/.vim/plugged')
"Plug 'govim/govim'
"Plug 'nikolvs/vim-sunbather'
call plug#end()
colorscheme sunbather
